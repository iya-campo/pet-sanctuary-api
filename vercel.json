{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*.ts", "tsconfig.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    },
    "commands": {
      "build": "prisma generate"
    }
  }
}